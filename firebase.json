{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Cache-Control",
        "value": "no-store"
      } ]
    } ]
  }
}
